          <mxGeometry x="315.50034482758633" y="75.00068965517244" width="64" height="64" as="geometry" />
        </mxCell>
        <mxCell id="nqpikjvwoDamXcm8Fm9y-11" value="&lt;h1 style=&quot;margin-top: 0px;&quot;&gt;Panoptes Extensibility &lt;/h1&gt;&lt;div&gt;Each module provides Panoptes with a method of identifying malicious files and processes on the system.&lt;/div&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;connectable=0;" parent="1" vertex="1">
          <mxGeometry x="526" y="902" width="264" height="100" as="geometry" />
